    Trata_Parametros = Err

    Select Case Err
        
        Case 65219
        
        Case 65218
            lErro = Rotina_Erro(vbOKOnly, "ERRO_RELATORIO_EXECUTANDO", Err)
        
        Case Else
            lErro = Rotina_Erro(vbOKOnly, "ERRO_FORNECIDO_PELO_VB", Err, Error$, 168947)

    End Select

    Exit Function

